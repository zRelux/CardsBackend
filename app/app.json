{
  "expo": {
    "name": "The Game With Funny Cards",
    "slug": "tgwfc",
    "owner": "relux",
    "privacy": "public",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "primaryColor": "#414141",
    "androidShowExponentNotificationInShellApp": false,
    "scheme": "tgwfc",
    "entryPoint": "./src/index.tsx",
    "icon": "./assets/icon.png",
    "assetBundlePatterns": ["**/*"],
    "description": "A multiplayer game with funny black humor cards",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#00000000",
      "hidden": false,
      "translucent": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "ios": {
      "bundleIdentifier": "com.thegamewithfunnycards",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "requireFullScreen": true,
      "isTabletOnly": false
    },
    "android": {
      "package": "com.thegamewithfunnycards",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/icon-fg.png"
      }
    }
  }
}
